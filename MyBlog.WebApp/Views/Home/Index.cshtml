@model HomeViewModel
@{
    ViewData["Title"] = "Home Page";
}

<div class="container mt-3">
    <div class="row">
        <div class="col-md-3">
            @await Component.InvokeAsync("CategoryMenu") 
        </div>
        <div class="col-md-9">
            @await Html.PartialAsync("_PartialSlider", Model.SliderNotes)

            @await Html.PartialAsync("_PartialNoteList", Model.HomeNotes)
        </div>

    </div>
</div>